<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="center.innovus.nutriassist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="center.innovus.nutriassist.MainActivity" />
        </activity>
        <activity android:name=".Activities.AsignarActivity" />
        <activity android:name=".Activities.PorcentajesComidasActivity" />
        <activity android:name=".Activities.ComidasActivity" />
        <activity android:name=".Activities.ComidasTabs" />
        <activity android:name=".Activities.ReporteActivity"></activity>
    </application>

</manifest>